@{
    ViewBag.Title = "Recaudacion";
    Layout = "~/Views/_LayoutPage.cshtml";
}
<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="card">
                <div class="card-header bg-primary">
                    <h3><i class="fa fa-institution" aria-hidden="true"></i> Recaudación FOSMAR-CORE</h3>
                </div>
                <div class="card-block">
                    <div class="row">
                        <div class="form-group col-sm-3">
                            <label class="control-label">Periodo</label>
                            <input type="number" id="txtAnio" class="control-form PopupCons" value="@ViewBag.Anio">
                        </div>
                        <div class="form-group col-sm-3 m-2">
                            <button class="input-group-inverse btn btn-inverse" id="btnConsultar" data-toggle="tooltip" data-placement="bottom" title="Consultar"><i class="fa fa-search fa-lg" aria-hidden="true"></i></button>
                            <button class="btn btn-out waves-effect waves-light btn-primary btn-square" id="btnCargarArchivo" data-toggle="tooltip" data-placement="bottom" title="Subir archivo Excel"><i class="fa fa-cloud-upload fa-lg" aria-hidden="true"></i> Cargar Excel</button>
                        </div>
                        <div class="row col-sm-1" style="text-align:left">
                            <span style="color: black; font-weight: bold; font-size: 17px;margin-top: 10px;">Total S/.</span>
                        </div>
                        <div class="row col-sm-4">
                            <span id="spnTotalRecaudacion" style="color:red;font-weight:bold;font-size:30px;text-align:right"></span>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-lg-12">
                            <div class="tab-header card">
                                <ul class="nav nav-tabs md-tabs tab-timeline" role="tablist" id="mytab">
                                    <li class="nav-item">
                                        <a class="nav-link active" id="btnTabBienes" data-toggle="tab" href="#items" role="tab">Cuentas por Cobrar</a>
                                        <div class="slide"></div>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" id="btnTabObras" data-toggle="tab" href="#items" role="tab">Recibos Pagados</a>
                                        <div class="slide"></div>
                                    </li>
                                </ul>
                            </div>
                            <div class="tab-content">
                                <div class="tab-pane active" id="items" role="tabpanel">
                                    <div class="card">
                                        <div class="card-block">
                                            <div class="form-row">
                                                <div class="form-group col-md-12">
                                                    <div class="table-responsive" id="divLista"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="form-row">
                        <div class="modal custom-modal" tabindex="-1" role="dialog" aria-hidden="true" id="divPopupContainer" style="overflow-y:auto;">
                            <div class="modal-dialog modal-xl bg-personalizado" id="divPopupWindow">
                                <div class="modal-content">
                                    <div id="divBarra" class="modal-header bg-personalizado" style="cursor:move;">
                                        <h5 id="titleModal" class="modal-title">Importar Archivo Excel</h5>
                                    </div>
                                    <div id="divCuerpo" class="modal-body">
                                        <div id="importarDataExcel">
                                            <div class="row">
                                                <div class="form-group col-sm-3" style="margin-top: 15px;margin-bottom: 2px;">
                                                    <button class="btn btn-out waves-effect waves-light btn-warning btn-square" id="btnLimpiar" data-toggle="tooltip" data-placement="bottom" title="Limpiar Registro"><i class="fa fa-paint-brush fa-lg" aria-hidden="true"></i>Limpiar</button>
                                                    &nbsp;&nbsp;&nbsp;
                                                    <button class="btn btn-out waves-effect waves-light btn-info btn-square" id="btnCargarCXC" data-toggle="tooltip" data-placement="bottom" title="Cargar Archivo"><i class="fa fa-upload fa-lg" aria-hidden="true"></i>Cargar</button>
                                                    <h4 id="spanPendiente" style="color:red"></h4>
                                                </div>
                                                <div class="form-group col-sm-6">
                                                    <label class="control-label">Entidad Financiera</label>
                                                    <select id="cboEntidadFinancieraCarga" class="control-form"></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="vizualizar">
                                            <div class="row">
                                                <div class="form-group col-sm-2">
                                                    <input type="hidden" class="control-form" id="txtIdRegistro">
                                                    <label class="control-label">Año Fiscal</label>
                                                    <input type="number" class="control-form" id="txtAnioEjecucion">
                                                </div>

                                                <div class="form-group col-sm-4">
                                                    <label class="control-label">Entidad Financiera</label>
                                                    <select id="cboEntidadFinanciera" class="control-form"></select>
                                                </div>
                                                <div class="form-group col-sm-2">
                                                    <label class="control-label">Codigo</label>
                                                    <input type="number" class="control-form" id="txtCodigo">
                                                </div>
                                                <div class="form-group col-sm-2">
                                                    <label class="control-label">Fecha Abono</label>
                                                    <input type="date" id="txtFechaAbono" autocomplete="off" class="control-form" />
                                                </div>

                                                <div class="form-group col-sm-2">
                                                    <button class="btn btn-out waves-effect waves-light btn-success btn-square" id="btnAprobar" data-toggle="tooltip" data-placement="bottom" title="Aprobar Recaudación"><i class="fa fa-thumbs-up fa-lg" aria-hidden="true"></i>Aprobar</button>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="form-group col-sm-3">
                                                    <label class="control-label">Total</label>
                                                    <input type="text" class="control-form" id="txtTotal">
                                                </div>
                                                <div class="form-group col-sm-3">
                                                    <label class="control-label">Estado</label>
                                                    <select id="cboEstado" class="control-form"></select>
                                                </div>
                                                <div class="form-group col-sm-4">
                                                    <label class="control-label">Nombre de Archivo</label>
                                                    <input type="text" class="control-form" id="txtArchivo">
                                                </div>
                                            </div>
                                        </div>


                                        <div class="form-row" style="height:350px;overflow-y:auto;">
                                            <div class="table-responsive" id="divListaExcel"></div>
                                        </div>


                                    </div>
                                    <div class="modal-footer">
                                        <button class="btnCancelarForm" id="btnCancelar" title="Cancelar"><i class="fa fa-undo" aria-hidden="true"></i>&nbsp;Cancelar</button>
                                        <button class="btnGuardarForm bg-customer" id="btnGuardar" title="Guardar"><span class="fa fa-save"></span>&nbsp;Guardar</button>
                                    </div>
                                    <div class="Fila">
                                        <span id="spnValidaGrabar" class="Valida"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal custom-modal" tabindex="-1" role="dialog" aria-hidden="true" id="divPopupContainerForm1" style="overflow-y:auto;">
                            <div class="modal-dialog bg-personalizado" id="divPopupWindowForm1">
                                <div class="modal-content">
                                    <div id="divBarraForm1" class="modal-header bg-personalizado" style="cursor:move;">
                                        <h5 class="modal-title">Cargar Archivo</h5>
                                    </div>
                                    <div id="divCuerpo" class="modal-body">
                                        <div class="form-row">
                                            <div class="col-sm-12" style="text-align:center">
                                                <input type="file" id="fupExcel" class="inputfile inputfile-5 Popup Requerido" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel,text/comma-separated-values, text/csv, application/csv" data-msg="Archivo a importar" data-multiple-caption="{count} files selected" multiple />
                                                <label for="fupExcel">
                                                    <figure><svg xmlns="http://www.w3.org/2000/svg" width="20" height="17" viewBox="0 0 20 17"><path d="M10 0l-5.2 4.9h3.3v5.1h3.8v-5.1h3.3l-5.2-4.9zm9.3 11.5l-3.2-2.1h-2l3.4 2.6h-3.5c-.1 0-.2.1-.2.1l-.8 2.3h-6l-.8-2.2c-.1-.1-.1-.2-.2-.2h-3.6l3.4-2.6h-2l-3.2 2.1c-.4.3-.7 1-.6 1.5l.6 3.1c.1.5.7.9 1.2.9h16.3c.6 0 1.1-.4 1.3-.9l.6-3.1c.1-.5-.2-1.2-.7-1.5z" /></svg></figure>
                                                    <span id="snpnombrearchivo">Seleccione archivo&hellip;</span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button class="btnCancelarForm" id="btnCancelarForm1" title="Cancelar"><i class="fa fa-undo" aria-hidden="true"></i>&nbsp;Cancelar</button>
                                        <button class="btnGuardarForm bg-customer" id="btnImportarExcel" title="Importar"><i class="fa fa-upload" aria-hidden="true"></i>&nbsp;Importar</button>
                                    </div>
                                    <div class="Fila">
                                        <span id="spnValida" class="Valida"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="styleSelector">
    </div>
</div>
<iframe name="print_frame" width="0" height="0" frameborder="0" src="about:blank"></iframe>
<script src="~/Riztie/Js/App/AppIngresos.js"></script>

