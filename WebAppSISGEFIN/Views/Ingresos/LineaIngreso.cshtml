
@{
    ViewBag.Title = "Linea Ingreso";
    Layout = "~/Views/_LayoutPage.cshtml";
}
<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="card">
                <div class="card-header bg-primary">
                    <h3><i class="fa fa-money" aria-hidden="true"></i> LINEA DE  INGRESOS</h3>
                </div>
                <div class="card-block">
                    <div class="form-row">
                        <div class="form-group col-sm-2">
                            <button class="input-group-inverse btn btn-primary" id="btnNuevo">Nuevo <i class="fa fa-plus-circle fa-lg" aria-hidden="true"></i></button>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <div class="table-responsive" id="divLista"></div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="modal custom-modal" tabindex="-1" role="dialog" aria-hidden="true" id="divPopupContainer" style="overflow-y:auto;">
                            <div class="modal-dialog modal-lg bg-personalizado" id="divPopupWindow" style="margin-top:30px;">
                                <div class="modal-content">
                                    <div id="divBarra" class="modal-header bg-personalizado" style="cursor:move">
                                        <h5 class="modal-title" id="tituloModal">Nuevo Registro</h5>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <input type="hidden" class="control-form Popup" id="txtIdRegistro">
                                                <label class="control-label">Nombre del Ingreso</label>
                                                <input type="text" autocomplete="off" class="control-form Reque Popup" maxlength="50" id="txtIngreso">
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-6">
                                                <label class="control-label">Abrevitura</label>
                                                <input type="text" autocomplete="off" class="control-form Reque Popup" maxlength="20" id="txtAbreviatura">
                                            </div>
                                            <div class="form-group col-sm-4">
                                                <label class="control-label">Estado</label>
                                                <select id="cboEstado" class="control-lectura Reque Popup"></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <fieldset class="fieldset">
                                                    <legend class="legend">Cuentas Cobrar</legend>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Código Cuenta por Cobrar </label>
                                                            <input type="text" autocomplete="off" class="control-form Popup" maxlength="10" id="txtCodigoCuentaxCC">
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Cuenta Mayor</label>
                                                            <select id="cboCuentaMayor" class="SelectSearch control-form Reque Popup"></select>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Sub Cuenta</label>
                                                            <select id="cboSubCuenta" class="SelectSearch control-form Reque Popup"></select>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Clasificador</label>
                                                            <select id="cboClasificador" class="SelectSearch control-form Reque Popup"></select>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                            <div class="col-sm-6">
                                                <fieldset class="fieldset">
                                                    <legend class="legend">Cuentas Abono</legend>
                                                    <div class="row">
                                                        <div class="form-group col-sm-6">
                                                            <label class="control-label">Código Cuenta por Abono </label>
                                                            <input type="text" autocomplete="off" class="control-form Popup" maxlength="10" id="txtCodigoCuentaAbono">
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Cuenta Mayor</label>
                                                            <select id="cboCuentaMayorAbono" class="SelectSearch control-form Reque Popup"></select>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Sub Cuenta</label>
                                                            <select id="cboSubCuentaAbono" class="SelectSearch control-form Reque Popup"></select>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="form-group col-sm-12">
                                                            <label class="control-label">Clasificador</label>
                                                            <select id="cboClasificadorAbono" class="SelectSearch control-form Reque Popup"></select>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button id="btnCancelar" class="btnCancelarForm" title="Cancelar"><i class="fa fa-undo" aria-hidden="true"></i>&nbsp;Cancelar</button>
                                        <button id="btnGuardar" class="btnGuardarForm bg-customer" title="Guardar datos"><span class="fa fa-save"></span>&nbsp;Grabar</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<iframe name="print_frame" width="0" height="0" frameborder="0" src="about:blank"></iframe>
<script src="~/Riztie/Js/App/AppIngresos.js"></script>

