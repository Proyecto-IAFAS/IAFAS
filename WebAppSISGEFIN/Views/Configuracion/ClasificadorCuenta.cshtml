
@{
    ViewBag.Title = "ClasificadorCuenta";
    Layout = "~/Views/_LayoutPage.cshtml";
}

<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="card">
                <div class="card-header bg-primary">
                    <h3><i class="fa fa-file" aria-hidden="true"></i> CATALOGO / CUENTA CONTABLE</h3>
                </div>
                <div class="card-block">
                    <div class="form-row">
                        <div class="form-group col-sm-10">
                            <button class="input-group-inverse btn btn-primary" id="btnNuevo">Nuevo <i class="fa fa-plus-circle fa-lg" aria-hidden="true"></i></button>
                            <button class="input-group-inverse btn btn-info" id="btnListado" style="display:none"> Ver Listado <i class="fa fa-list" aria-hidden="true"></i> </button>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <div class="table-responsive" id="divLista"></div>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="modal custom-modal" tabindex="-1" role="dialog" aria-hidden="true" id="divPopupContainer" style="overflow-y:auto;">
                            <div class="modal-dialog modal-lg bg-personalizado" id="divPopupWindow" style="margin-top:30px;">
                                <div class="modal-content">
                                    <div id="divBarra" class="modal-header bg-personalizado" style="cursor:move">
                                        <h5 class="modal-title" id="tituloModal"></h5>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="form-group col-sm-4">
                                                <input type="hidden" class="control-form Popup">
                                                <label class="control-label">Entidad</label>
                                                <select id="cboEntidad" class="control-form Reque Popup"></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-4">
                                                <label class="control-label">Tipo de Bien</label>
                                                <select id="cboTipoBien" class="control-form Reque Popup"></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Grupo</label>
                                                <select id="cboGrupo" class="SelectSearch control-form Reque Popup"></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Clase</label>
                                                <select id="cboClase" class="SelectSearch control-form Popup"></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Familia</label>
                                                <select id="cboFamilia" class="SelectSearch control-form Popup"></select>
                                            </div>
                                        </div>
                                        <div class="form-row">
                                            <div class="form-group col-sm-12">
                                                <div class="table-responsive" id="divListaCtaContable"></div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="modal-footer">
                                        <button id="btnCancelar" class="btnCancelarForm" title="Cancelar"><i class="fa fa-undo" aria-hidden="true"></i>&nbsp;Cancelar</button>
                                        <button id="btnGuardar" class="btnGuardarForm bg-customer" title="Guardar datos"><span class="fa fa-save"></span>&nbsp;Grabar</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal custom-modal" tabindex="-1" role="dialog" aria-hidden="true" id="divPopupContainerForm1" style="overflow-y:auto;">
                            <div class="modal-dialog modal-lg bg-personalizado" id="divPopupWindowForm1" style="margin-top:30px;">
                                <div class="modal-content">
                                    <div id="divBarraForm1" class="modal-header bg-personalizado" style="cursor:move">
                                        <h5 class="modal-title" id="tituloModal"></h5>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="form-group col-sm-4">
                                                <input id="txtIdRegistroEdit" type="hidden" class="control-lectura RequeEdit PopupEdit">
                                                <label class="control-label">Entidad</label>
                                                <select id="cboEntidadEdit" class="control-lectura RequeEdit PopupEdit" disabled></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-4">
                                                <label class="control-label">Tipo de Bien</label>
                                                <select id="cboTipoBienEdit" class="control-lectura RequeEdit PopupEdit" disabled></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Grupo</label>
                                                <select id="cboGrupoEdit" class="control-lectura RequeEdit PopupEdit" disabled></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Clase</label>
                                                <select id="cboClaseEdit" class="control-lectura RequeEdit PopupEdit" disabled></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Familia</label>
                                                <select id="cboFamiliaEdit" class="control-lectura RequeEdit PopupEdit" disabled></select>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group col-sm-10">
                                                <label class="control-label">Cuenta Contable</label>
                                                <select id="cboCtaContableEdit" class="SelectSearch control-form RequeEdit PopupEdit"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button id="btnCancelarEdicion" class="btnCancelarForm" title="Cancelar"><i class="fa fa-undo" aria-hidden="true"></i>&nbsp;Cancelar</button>
                                        <button id="btnGuardarEdicion" class="btnGuardarForm bg-customer" title="Guardar datos"><span class="fa fa-save"></span>&nbsp;Grabar</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="styleSelector">
        </div>
    </div>
</div>
<iframe name="print_frame" width="0" height="0" frameborder="0" src="about:blank"></iframe>
<script src="~/Riztie/Js/App/AppConfig.js"></script>

