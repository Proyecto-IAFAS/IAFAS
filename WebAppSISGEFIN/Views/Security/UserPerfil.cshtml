@{
    ViewBag.Title = "UserPerfil";
    Layout = "~/Views/_LayoutPage.cshtml";
}
<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">

                    <div class="row">
                        <div class="col-lg-12">
                            <div class="tab-header card">
                                <ul class="nav nav-tabs md-tabs tab-timeline" role="tablist" id="mytab">
                                    <li class="nav-item">
                                        <a class="nav-link active" data-toggle="tab" href="#personal" role="tab">Información Personal</a>
                                        <div class="slide"></div>
                                    </li>
                                </ul>
                            </div>
                            <div class="tab-content">
                                <div class="tab-pane active" id="personal" role="tabpanel">
                                    <div class="card">
                                        <div class="card-block">
                                            <div class="edit-info">
                                                <div id="divValida" style="display:none" class="alert background">
                                                    <span class="closebtn">&times;</span>
                                                    <strong>Advertencia!</strong><br />Los campos remarcados en rojo son obligatorios.
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <div class="general-info form-material">
                                                            <div class="row">
                                                                <fieldset class="col-md-12">
                                                                    <div class="row">
                                                                        <div class="col-lg-9">
                                                                            <div class="row">
                                                                                <div class="col-lg-4">
                                                                                    <div class="form-group">
                                                                                        <label class="control-label" for="txtNroDocumento">Número Documento</label>
                                                                                        <div class="input-group">
                                                                                            <input type="hidden" class="control-form Popup Requerido" id="txtIdPersona" autocomplete="off">
                                                                                            <input type="text" class="control-lectura Popup Requerido" id="txtNroDocumento" autocomplete="off" readonly>
                                                                                           </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-lg-6">
                                                                                    <div class="form-group">
                                                                                        <label class="control-label" for="nombres">Nombres</label>
                                                                                        <div class="input-group">
                                                                                            <input type="text" class="control-lectura Popup Grabar Requerido" id="txtNombre" autocomplete="off" required="" readonly>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="row">
                                                                                <div class="col-lg-4">
                                                                                    <div class="form-group">
                                                                                        <label class="control-label" for="cboDepartamento">Perfil</label>
                                                                                        <div class="input-group">
                                                                                            <input type="text" class="control-lectura Popup Requerido" id="txtDepartamento" autocomplete="off">
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </fieldset>
                                                            </div>

                                                            <br />
                                                            <div class="row">
                                                                <fieldset class="col-md-12">
                                                                    <legend class="legend">Cambio de contraseña</legend>
                                                                    <div class="row">
                                                                        <div class="col-lg-4">
                                                                            <div class="form-group form-primary">
                                                                                <label class="control-label">Nueva Contraseña</label>
                                                                                <input type="text" id="txtContrasenia" class="control-form">
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-4">
                                                                            <div class="form-group form-primary">
                                                                                <label class="control-label">Confirmar Contraseña</label>
                                                                                <input type="text" id="txtConfirmarContrasenia" class="control-form">
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-4">
                                                                            <div class="form-group form-primary">
                                                                                <label class="control-label">&nbsp;</label>
                                                                                <button class="btnGuardarForm bg-customer" id="btnGrabarSeguridad" title="Guardar"><span class="fa fa-save"></span>&nbsp;Guardar</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </fieldset>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<iframe name="print_frame" width="0" height="0" frameborder="0" src="about:blank"></iframe>
<script src="~/Daymeva/Js/App/AppSecurity.js"></script>

